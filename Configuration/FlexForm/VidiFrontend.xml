<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<settings.dataType>
				<TCEforms>
					<label>LLL:EXT:vidi_frontend/Resources/Private/Language/locallang.xlf:flexform.content_type</label>
					<onChange>reload</onChange>
					<config>
						<type>select</type>
						<itemsProcFunc>Fab\VidiFrontend\Backend\TceForms->feedItemsForSettingsDataTypes</itemsProcFunc>
						<size>1</size>
						<maxitems>1</maxitems>
					</config>
				</TCEforms>
			</settings.dataType>
			<settings.templateDetail>
				<TCEforms>
					<label>LLL:EXT:vidi_frontend/Resources/Private/Language/locallang.xlf:flexform.template_for_detail_view</label>
					<config>
						<type>select</type>
						<itemsProcFunc>Fab\VidiFrontend\Backend\TceForms->feedItemsForSettingsTemplateShow</itemsProcFunc>
						<size>1</size>
						<maxitems>1</maxitems>
					</config>
				</TCEforms>
			</settings.templateDetail>
			<settings.isVisualSearchBar>
				<TCEforms>
					<label>LLL:EXT:vidi_frontend/Resources/Private/Language/locallang.xlf:flexform.is_visual_search_bar</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</settings.isVisualSearchBar>
			<settings.defaultNumberOfItems>
				<TCEforms>
					<label>LLL:EXT:vidi_frontend/Resources/Private/Language/locallang.xlf:flexform.number_of_items</label>
					<config>
						<type>input</type>
						<default>10, 20, 50, 100, -1</default>
					</config>
				</TCEforms>
			</settings.defaultNumberOfItems>
			<settings.columns>
				<TCEforms>
					<label>LLL:EXT:vidi_frontend/Resources/Private/Language/locallang.xlf:flexform.columns</label>
					<config>
						<type>select</type>
						<itemsProcFunc>Fab\VidiFrontend\Backend\TceForms->feedItemsForSettingsColumns</itemsProcFunc>
						<size>10</size>
						<maxitems>100</maxitems>
					</config>
				</TCEforms>
			</settings.columns>
		</el>
	</ROOT>
</T3DataStructure>